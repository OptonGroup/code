// MXCODE
#define _CRT_SECURE_NO_WARNINGS
#include <iostream>
#include <vector>
#include <algorithm>
#include <functional>
#include <cctype>
#include <string>
#include <set>
#define endl '\n'
using namespace std;

vector <vector <int>> g;
vector <int> color_v;
int n, m, ans = 0;

void dfs(int i, int color) {
	if (ans == -1)
		return;
	color_v[i] = color;
	for (int u : g[i]) {
		if (!color_v[u])
			dfs(u, 3 - color);
		else {
			ans = -1;
		}
	}
}

int main() {
#ifndef ONLINE_JUDGE
	freopen("input.txt", "r", stdin); freopen("output.txt", "w", stdout);
#endif // !ONLINE_JUDGE
	ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);

	cin >> n >> m;
	g.resize(n, vector<int>());
	color_v.resize(n);
	int a, b;
	set <int> v;
	for (int i = 0; i < m; ++i) {
		cin >> a >> b;
		g[a - 1].push_back(b);
		g[b - 1].push_back(a);
	}

	for (int i = 0; i < n; ++i) {
		if (!color_v[i])
			dfs(i, 1);
	}
	
	// Надо выводить только те экзамены, которые были введены 

	return 0;
}
